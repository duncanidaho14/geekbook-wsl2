{% extends 'base.html.twig' %}

{% block title %}Accueil !{% endblock %}

{% block body %}
<style>
.gradient {
        background: linear-gradient(90deg, #d53369 0%, #daae51 100%);
      }
</style>

<div class="body leading-normal tracking-normal text-white gradient" style="font-family: 'Source Sans Pro', sans-serif;">
    <div class="pt-24 dark:bg-gray-800 dark:text-white">
      <div class="container px-3 mx-auto flex flex-wrap flex-col md:flex-row items-center">
        <!--Left Col-->
        <div class="flex flex-col w-full md:w-2/5 justify-center items-start text-center md:text-left">
          <p class="uppercase tracking-loose w-full">Quel livre voulez-vous acheter ?</p>
          <h1 class="my-4 text-5xl font-bold leading-tight">
            GeekBook écrit par vous, lu par nous ...
          </h1>
          <p class="leading-normal text-2xl mb-8">
            Et écrit par nous et lu par vous !
          </p>
          <button class="mx-auto lg:mx-0 hover:underline bg-white text-gray-800 font-bold rounded-full my-6 py-4 px-8 shadow-lg focus:outline-none focus:shadow-outline transform transition hover:scale-105 duration-300 ease-in-out">
            Les livres à lire !!!
          </button>
        </div>
        <!--Right Col-->
        <div class="w-full md:w-3/5 py-6 text-center">
          <img class="w-full md:w-4/5 z-50" src="{{ asset('build/images/hero.png') }}" alt="livre et recherche sur l'ordinateur" />
        </div>
      </div>
    </div>
    <div class="relative -mt-12 lg:-mt-24">
