    <section class="border-b py-8 dark:bg-gray-800 dark:text-white">
      <div class="container max-w-5xl mx-auto m-8">
        <h2 class="w-full my-2 text-5xl font-bold leading-tight text-center text-gray-800">
          Les Livres
        </h2>
        <div class="w-full mb-4">
          <div class="h-1 mx-auto gradient w-64 opacity-25 my-0 py-0 rounded-t"></div>
        </div>
        <div class="container m-2">
          <div class="flex flex-wrap hover:flex-wrap-reverse">
            <!--cards    -->
            {% for book in books %}
              <div class="max-w-sm  basis-1/4 p-2 rounded overflow-hidden shadow-lg">
                <a href="{{path('app_book_show', {'slug': book.slug })}}" alt="">
                  <img class="w-full" src="{{book.url}}" alt="{{book.name}}">
                </a>
                <div class="px-6 py-4">
                  <div class="font-bold text-xl mb-2">{{book.title}}</div>
                  <p class="text-gray-700 text-base">
                    {{book.introduction}}
                  </p>
                </div>
                
                  <div class="px-6 pt-4 pb-2">
                    <span class="inline-block bg-purple-200 rounded-full px-3 py-1 text-sm font-semibold text-red-700 mr-2 mb-2"># {{book.catName}}</span>
                    <img src="{{book.image}}" alt="{{book.catName}}" class="w-6 h-6 inline-block" />
                  </div>
              </div>
              <hr>
            {% endfor %}
          </div>
          <hr> 
        </div>
        <div class="flex flex-wrap flex-col-reverse sm:flex-row">
          
        </div>
      </div>
    </section>

    <section class="bg-white dark:bg-gray-800 dark:text-white border-b py-8">
      <div class="container max-w-5xl mx-auto m-8">
        <h2 class="w-full my-2 text-5xl font-bold leading-tight text-center text-gray-800">
          Les Livres
        </h2>
        <div class="w-full mb-4">
          <div class="h-1 mx-auto gradient w-64 opacity-25 my-0 py-0 rounded-t"></div>
        </div>
        <div class="container m-2">
          <div class="flex flex-wrap hover:flex-wrap-reverse">
            <!--cards    -->
            {% for bookr in booksRepo %}
            <div class="max-w-sm  basis-1/4 p-2 rounded overflow-hidden shadow-lg">
              <a href="{{path('app_book_show', {'slug': bookr.slug })}}" alt="">
                {% for img in bookr.images %}
                  <img class="w-full" src="{{img.url}}" alt="{{img.name}}">
                {% endfor %}
              </a>
              <div class="px-6 py-4">
                <div class="font-bold text-xl mb-2">{{bookr.title}}</div>
                <p class="text-gray-700 text-base">
                  {{bookr.description}}
                </p>
              </div>   
              <div class="px-6 pt-4 pb-2">
                {% for bookc in bookr.categories %}
                  <span class="inline-block bg-purple-200 rounded-full px-3 py-1 text-sm font-semibold text-red-700 mr-2 mb-2"># {{bookc.name}}</span>
                  <img src="{{bookc.image}}" alt="{{bookc.name}}" class="w-6 h-6 inline-block" />
                {% endfor %}
              </div>
            </div>
          <hr />
          {% endfor %}
        </div>
          <hr>
          
        </div>
        <div class="flex flex-wrap flex-col-reverse sm:flex-row">
          
        </div>
      </div>
    </section>


    <section class="bg-white dark:bg-gray-800 dark:text-white border-b py-8">
      <div class="container mx-auto flex flex-wrap pt-4 pb-12">
        <h2 class="w-full my-2 text-5xl font-bold leading-tight text-center text-gray-800 dark:bg-gray-800 dark:text-white">
          Les catégories
        </h2>
        {% for category in categories %}
        <div class=" mb-4">
          <div class="h-1 mx-auto gradient w-64 opacity-25 my-0 py-0 rounded-t"></div>
        </div>
        <div class="w-full md:w-1/3 p-6 flex flex-col flex-grow flex-shrink">
          <div class="flex-1 bg-white rounded-t rounded-b-none overflow-hidden shadow">
            <a href="#" class="flex flex-wrap no-underline hover:no-underline">
              <h3 class=" text-gray-600 text-xs md:text-sm px-6">
                <a href="{{path('app_category_show', {'id': category.id })}}">{{category.name}}</a>
              </h3>
              <div class="w-full font-bold text-xl text-gray-800 px-6">
                <img src="{{category.image}}" alt="{{category.name}}" class="w-25 h-15"/>
              </div>
              <p class="text-gray-800 text-base px-6 mb-5">
                
              </p>
            </a>
          </div>
          <div class="flex-none mt-auto bg-white rounded-b rounded-t-none overflow-hidden shadow p-6">
            <div class="flex items-center justify-start">
              <button class="mx-auto lg:mx-0 hover:underline gradient text-white font-bold rounded-full my-6 py-4 px-8 shadow-lg focus:outline-none focus:shadow-outline transform transition hover:scale-105 duration-300 ease-in-out">
                <a href="{{path('app_category_show', {'id': category.id})}}">{{category.name}}</a>
              </button>
            </div>
          </div>
        </div>
        {% endfor %}
      </div>
    </section>
    <section class="bg-gray-100 dark:bg-gray-800 dark:text-white py-8">
      <div class="container mx-auto px-2 pt-4 pb-12 text-gray-800">
        <h2 class="w-full my-2 text-5xl font-bold leading-tight text-center text-gray-800 dark:text-white">
          Les auteurs
        </h2>
        <div class="w-full mb-4">
          <div class="h-1 mx-auto gradient w-64 opacity-25 my-0 py-0 rounded-t"></div>
        </div>
        
        <div class="font-sans antialiased text-gray-900 leading-normal tracking-wider bg-cover">
          {% for author in authors %}

